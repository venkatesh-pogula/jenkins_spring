node ('node_java_11') {
        stage ('scm'){
            git branch:'scripted', url:'https://github.com/venkatesh-pogula/jenkins_spring.git'
        }
        stage ('build'){
            sh ' mvn clean package'
        }
        post{
        sucess {
            archive '**/*.jar'
            junit '**/TEST-*.xml'

            }
       } 
}       
