node ('node_java_11') {
        stage ('scm'){
            git branch:'scripted', url:'https://github.com/venkatesh-pogula/jenkins_spring.git'
        }
        stage ('build'){
            sh ' mvn clean package'
        }
        stage('Archive artifact'){
          archiveArtifacts artifacts: '***/*.jar', followSymlinks: false
        }
        stage('Junit Test'){
          junit '*.xml'
        }
        
}       
